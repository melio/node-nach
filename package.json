{
  "name": "node-nach",
  "description": "nACH is a highly customizable Node.js module exposing a high & low-level API for generating ACH files for use within the ACH network",
  "version": "1.2.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/melio/node-nach.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --timeout 10000 --reporter spec --recursive",
    "achToJson": "node tools.js $1",
    "release": "release-it"
  },
  "dependencies": {
    "async": "^2.0.1",
    "lodash": "^4.14.1",
    "moment": "^2.22.0"
  },
  "devDependencies": {
    "chai": "latest",
    "mocha": "latest",
    "release-it": "^14.2.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/melio"
  },
  "release-it": {
    "npm": {
      "publish": false
    }
  }
}
